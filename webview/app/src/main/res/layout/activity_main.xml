<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/clearCache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clear Cache"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/LoadCacheElseNetwork"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Load From Cache Else Network"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/LoadFromCache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Load From Cache"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/LoadDefault"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Load Default"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/GoToGoogle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go To Google"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="330dp" />
        <WebView
            android:id="@+id/webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            />

</LinearLayout>